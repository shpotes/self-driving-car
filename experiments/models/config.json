{
    "model" : {
        "backend":              "SqueezeNet",
        "input_size":           416,
        "anchors":              [0.57273, 0.677385, 1.87446, 2.06253, 3.33843, 5.47434, 7.88282, 3.52778, 9.77052, 9.16828],
        "max_box_per_image":    50,        
        "labels":               ["left_arrow", "person", "right_arrow", "sem", "stop"],
        "actions":              ["forward_left", "forward", "forward_right", "backward_left", "backward", "backward_right"]
    },

    "train": {
        "train_image_folder":   "dataset/images/",
        "train_annot_folder":   "dataset/annotations/",

        "validation_split" :    0.8,
        
        "batch_size":           16,
        "learning_rate":        1e-4,
        "nb_epochs":            20,

        "object_scale":         5.0 ,
        "no_object_scale":      1.0,
        "coord_scale":          1.0,
        "class_scale":          1.0,

        "saved_weights_name":   "vehicle.h5",
        "debug":                true
    }
}
